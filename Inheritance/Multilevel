//Multilevel Inheritance Example
class grandfather{
     char gender = 'M';
    void print(){
        System.out.println("hello");
    }
}
class father extends grandfather{
   
}
class son extends father{
    
}
class Main{
    public static void main(String[] args){
        son s = new son();
        System.out.println(s.gender);
        s.print();
    }
}
